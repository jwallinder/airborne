<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools" android:layout_width="match_parent"
    android:layout_height="match_parent" android:orientation="vertical"
    tools:context=".MainActivity" tools:deviceIds="wear_square">

    <TextView android:id="@+id/airborne_heading" android:layout_width="wrap_content"
        android:layout_height="wrap_content" android:text="@string/airborne_heading" />
    <TextView android:id="@+id/airborne_time" android:layout_width="wrap_content"
        android:layout_height="wrap_content" android:text="@string/airborne_time" />

    <TextView android:id="@+id/accelerometer" android:layout_width="wrap_content"
        android:layout_height="wrap_content" android:text="@string/accelorometer" />
    <TextView android:id="@+id/accX" android:layout_width="wrap_content"
        android:layout_height="wrap_content" android:text="@string/hello_square" />
    <TextView android:id="@+id/accY" android:layout_width="wrap_content"
        android:layout_height="wrap_content" android:text="@string/hello_square" />
    <TextView android:id="@+id/accZ" android:layout_width="wrap_content"
        android:layout_height="wrap_content" android:text="@string/hello_square" />
    <TextView android:id="@+id/accG" android:layout_width="wrap_content"
        android:layout_height="wrap_content" android:text="@string/hello_square" />
    <TextView android:id="@+id/minMaxG" android:layout_width="wrap_content"
        android:layout_height="wrap_content" android:text="@string/hello_square" />

    <TextView android:id="@+id/gravity" android:layout_width="wrap_content"
        android:layout_height="wrap_content" android:text="@string/gravity" />
    <TextView android:id="@+id/text" android:layout_width="wrap_content"
        android:layout_height="wrap_content" android:text="@string/hello_square" />
    <TextView android:id="@+id/gravX" android:layout_width="wrap_content"
        android:layout_height="wrap_content" android:text="@string/hello_square" />
    <TextView android:id="@+id/gravY" android:layout_width="wrap_content"
        android:layout_height="wrap_content" android:text="@string/hello_square" />
    <TextView android:id="@+id/gravZ" android:layout_width="wrap_content"
        android:layout_height="wrap_content" android:text="@string/hello_square" />
    <TextView android:id="@+id/gravG" android:layout_width="wrap_content"
        android:layout_height="wrap_content" android:text="@string/hello_square" />


</LinearLayout>
